  File                     = {:Morin2008.pdf:PDF},
  Institution              = {Genome Sciences Centre, BC Cancer Agency, Vancouver, British Columbia, Canada.},
  Keywords                 = {DNA, Complementary, genetics; Exons; Gene Expression Profiling; Hela Cells; Humans; Polymorphism, Single Nucleotide; RNA Splicing; RNA, Messenger, analysis; Transcription Initiation Site, rna-seq old hela method},
  Language                 = {eng},

@Article{Madsen2005,
  Title                    = {{A}n exonic splicing silencer downstream of the 3' splice site {A2} is required for efficient human immunodeficiency virus type 1 replication},
  Author                   = {Joshua M Madsen and C. Martin Stoltzfus},
  Journal                  = {J Virol},

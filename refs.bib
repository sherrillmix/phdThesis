  Timestamp                = {2015.05.06 16:08},
  Url                      = {http://dx.doi.org/10.1056/NEJM199610103351501}
}

@Article{Hammer1997,
  Title                    = {{A} controlled trial of two nucleoside analogues plus indinavir in persons with human immunodeficiency virus infection and {CD}4 cell counts of 200 per cubic millimeter or less},

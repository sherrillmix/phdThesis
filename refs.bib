  Pii                      = {S0092-8674(08)00270-5},
  Pmid                     = {18329373},
  Timestamp                = {2013.01.31},
  Url                      = {http://dx.doi.org/10.1016/j.cell.2008.02.022}
}

@Article{Schopman2012,

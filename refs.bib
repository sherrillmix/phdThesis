  Keywords                 = {Amino Acids, metabolism; Chromatin Immunoprecipitation; Databases, Genetic; Gene Expression Profiling; Genes, Fungal; Genes, cdc; Genes, physiology; Humans; Models, Statistical; Oligonucleotide Array Sequence Analysis; Saccharomycetales, genetics; Vocabulary, Controlled, GO ontology analysis comparison gene condense tool webapp network category subset generative probalistic model},
  Language                 = {eng},

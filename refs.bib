  Author                   = {Kalish, Marcia L. and Wolfe, Nathan D. and Ndongmo, Clement B. and McNicholl, Janet and Robbins, Kenneth E. and Aidoo, Michael and Fonjungo, Peter N. and Alemnji, George and Zeh, Clement and Djoko, Cyrille F. and Mpoudi-Ngole, Eitel and Burke, Donald S. and Folks, Thomas M.},
  Journal                  = {Emerg Infect Dis},
  Year                     = {2005},

  Month                    = {Dec},
  Number                   = {12},
  Pages                    = {1928--1930},

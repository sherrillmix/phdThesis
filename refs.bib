@Article{Pan2006,
  Title                    = {{Q}uantitative microarray profiling provides evidence against widespread coupling of alternative splicing with nonsense-mediated m{RNA} decay to control gene expression},
  Author                   = {Qun Pan and Arneet L Saltzman and Yoon Ki Kim and Christine Misquitta and Ofer Shai and Lynne E Maquat and Brendan J Frey and Benjamin J Blencowe},

@Article{Kohl1988,
  Title                    = {{A}ctive human immunodeficiency virus protease is required for viral infectivity},
  Author                   = {Kohl, N. E. and Emini, E. A. and Schleif, W. A. and Davis, L. J. and Heimbach, J. C. and Dixon, R. A. and Scolnick, E. M. and Sigal, I. S.},

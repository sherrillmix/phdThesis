@Article{Yukl2009,
  Title                    = {{L}atently-infected {CD4}+ {T} cells are enriched for {HIV}-1 {Tat} variants with impaired transactivation activity},
  Author                   = {Steven Yukl and Satish Pillai and Peilin Li and Karen Chang and William Pasutti and Chris Ahlgren and Diane Havlir and Matthew Strain and Huldrych Günthard and Douglas Richman and Andrew P Rice and Eric Daar and Susan Little and Joseph K Wong},
  Journal                  = {Virology},
  Year                     = {2009},


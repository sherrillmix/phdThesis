@Article{Yu2005,
  Title                    = {{T}he long terminal repeat ({LTR}) of {ERV-9} human endogenous retrovirus binds to {NF-Y} in the assembly of an active {LTR} enhancer complex {NF-Y}/{MZF1}/{GATA-2}},

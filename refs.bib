@Article{Farnet1991,
  Title                    = {{D}etermination of viral proteins present in the human immunodeficiency virus type 1 preintegration complex},
  Author                   = {Farnet, C. M. and Haseltine, W. A.},
  Journal                  = {J Virol},
  Year                     = {1991},


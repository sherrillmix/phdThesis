  Owner                    = {shescott},
  Pii                      = {btm525},
  Pmid                     = {18006551},
  Timestamp                = {2010.01.07},
  Url                      = {http://dx.doi.org/10.1093/bioinformatics/btm525}
}

@Article{Ling2002,

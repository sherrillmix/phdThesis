@Article{Ward1986,
  Title                    = {{L}aboratory and epidemiologic evaluation of an enzyme immunoassay for antibodies to {HTLV}-{III}},
  Author                   = {Ward, J. W. and Grindon, A. J. and Feorino, P. M. and Schable, C. and Parvin, M. and Allen, J. R.},

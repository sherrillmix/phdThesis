@Article{Aebi1986,
  Title                    = {{S}equence requirements for splicing of higher eukaryotic nuclear pre-m{RNA}},
  Author                   = {M. Aebi and H. Hornig and R. A. Padgett and J. Reiser and C. Weissmann},
  Journal                  = {Cell},
  Year                     = {1986},


@Article{Maness2010,
  Title                    = {{CD}8+ {T} cell recognition of cryptic epitopes is a ubiquitous feature of {AIDS} virus infection},
  Author                   = {Maness, Nicholas J. and Walsh, Andrew D. and Piaskowski, Shari M. and Furlott, Jessica and Kolar, Holly L. and Bean, Alexander T. and Wilson, Nancy A. and Watkins, David I.},
  Journal                  = {J Virol},
  Year                     = {2010},


@Article{Sureau2001,
  Title                    = {{SC35} autoregulates its expression by promoting splicing events that destabilize its m{RNA}s},
  Author                   = {A. Sureau and R. Gattoni and Y. Dooghe and J. Stévenin and J. Soret},
  Journal                  = {EMBO J},

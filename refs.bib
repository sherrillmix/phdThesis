  Author                   = {Pfeffer, S{\'{e}}bastien and Sewer, Alain and Lagos-Quintana, Mariana and Sheridan, Robert and Sander, Chris and Gr{\"{a}}sser, Friedrich A. and {van Dyk}, Linda F. and Ho, C Kiong and Shuman, Stewart and Chien, Minchen and Russo, James J. and Ju, Jingyue and Randall, Glenn and Lindenbach, Brett D. and Rice, Charles M. and Simon, Viviana and Ho, David D. and Zavolan, Mihaela and Tuschl, Thomas},
  Journal                  = {Nat Methods},

}

@Article{Silva2010,
  Title                    = {{F}itness epistasis and constraints on adaptation in a human immunodeficiency virus type 1 protein region},
  Author                   = {Jack da Silva and Mia Coetzer and Rebecca Nedellec and Cristina Pastore and Donald E Mosier},
  Journal                  = {Genetics},
  Year                     = {2010},

  Month                    = {May},
  Number                   = {1},
  Pages                    = {293--303},
  Volume                   = {185},

  Abstract                 = {Fitness epistasis, the interaction among alleles at different loci in their effects on fitness, has potentially important consequences for adaptive evolution. We investigated fitness epistasis among amino acids of a functionally important region of the human immunodeficiency virus type 1 (HIV-1) exterior envelope glycoprotein (gp120). Seven mutations putatively involved in the adaptation of the second conserved to third variable protein region (C2-V3) to the use of an alternative host-cell chemokine coreceptor (CXCR4) for cell entry were engineered singly and in combinations on the wild-type genetic background and their effects on viral infectivity were measured. Epistasis was found to be common and complex, involving not only pairwise interactions, but also higher-order interactions. Interactions could also be surprisingly strong, changing fitness by more than 9 orders of magnitude, which is explained by some single mutations being practically lethal. A consequence of the observed epistasis is that many of the minimum-length mutational trajectories between the wild type and the mutant with highest fitness on cells expressing the alternative coreceptor are selectively inaccessible. These results may help explain the difficulty of evolving viruses that use the alternative coreceptor in culture and the delayed evolution of this phenotype in natural infection. Knowledge of common, complex, and strong fitness interactions among amino acids is necessary for a full understanding of protein evolution.},
  Doi                      = {10.1534/genetics.109.112458},
  File                     = {:Silva2010.pdf:PDF},
  Institution              = {School of Molecular and Biomedical Science, The University of Adelaide, Adelaide, South Australia 5005, Australia. jack.dasilva@adelaide.edu.au},
  Keywords                 = {Adaptation, Physiological, genetics; Amino Acid Sequence; Epistasis, Genetic; Evolution, Molecular; Genetic Fitness; HIV Envelope Protein gp120, chemistry/genetics; HIV-1, genetics; Humans; Molecular Sequence Data; Mutation, genetics; Receptors, CCR5, metabolism; Receptors, CXCR4, metabolism, HIV epistasis mutation CCR5 CXCR4 coreceptor switching CD4 fitness evolution engineer},
  Language                 = {eng},
  Medline-pst              = {ppublish},
  Owner                    = {scott},
  Pii                      = {genetics.109.112458},
  Pmid                     = {20157005},
  Timestamp                = {2011.11.29},
  Url                      = {http://dx.doi.org/10.1534/genetics.109.112458}
}

@Article{Simmons2001,
  Title                    = {{N}ef triggers a transcriptional program in {T} cells imitating single-signal {T} cell activation and inducing {HIV} virulence mediators},
  Author                   = {Simmons, A. and Aluvihare, V. and McMichael, A.},
  Journal                  = {Immunity},
  Year                     = {2001},

  Month                    = {Jun},
  Number                   = {6},
  Pages                    = {763--777},
  Volume                   = {14},

  Abstract                 = {Gene expression profiling was used to explore the role of Nef in HIV. Nef induces a transcriptional program in T cells that is 97\% identical to that of anti-CD3 T cell activation. This program is inhibited in the presence of cyclosporin. A requirement for TCR zeta and ZAP-70 is demonstrated for formation of the complete profile. Among eight factors particular to the anti-CD3 activation profile are IL16 and YY1, negative regulators of HIV transcription. In contrast, Nef exclusively upregulates factors positively regulating HIV, including Tat-SF1, U1 SNRNP, and IRF-2. New genes associated with Nef include CDK9, the induction of which enhances Tat function. Thus, Nef acts as a master switch early in the viral life cycle, forcing an environment conducive to dynamic viral production.},

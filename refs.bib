  Owner                    = {scott},
  Pii                      = {06-PLPA-RA-0426R2},
  Pmid                     = {17257061},
  Timestamp                = {2012.02.03},
  Url                      = {http://dx.doi.org/10.1371/journal.ppat.0030010}
}

@Article{Lefebvre2011,

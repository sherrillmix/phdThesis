@Article{Schones2008,
  Title                    = {{D}ynamic regulation of nucleosome positioning in the human genome},
  Author                   = {Dustin E Schones and Kairong Cui and Suresh Cuddapah and Tae-Young Roh and Artem Barski and Zhibin Wang and Gang Wei and Keji Zhao},
  Journal                  = {Cell},

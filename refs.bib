@Article{Wolfe2004,
  Title                    = {{E}xposure to nonhuman primates in rural {C}ameroon},
  Author                   = {Wolfe, Nathan D. and Prosser, Tassy A. and Carr, Jean K. and Tamoufe, Ubald and Mpoudi-Ngole, Eitel and Torimiro, J Ndongo and LeBreton, Matthew and McCutchan, Francine E. and Birx, Deborah L. and Burke, Donald S.},

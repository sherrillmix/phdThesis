  Keywords                 = {Algorithms; Gene Expression Profiling, methods; Models, Genetic; Oligonucleotide Array Sequence Analysis; Programming Languages; Software mgsa GO analysis tool R bioconductor package gene set},
  Language                 = {eng},
  Medline-pst              = {ppublish},
  Owner                    = {scott},
  Pii                      = {btr296},
  Pmid                     = {21561920},
  Timestamp                = {2012.09.10},
  Url                      = {http://dx.doi.org/10.1093/bioinformatics/btr296}
}

@Article{Beckmann1991,

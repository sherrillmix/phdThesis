@Article{Wu2008,
  Title                    = {{T}ranscriptional profiles in {CD8}+ {T} cells from {HIV}+ progressors on {HAART} are characterized by coordinated up-regulation of oxidative phosphorylation enzymes and interferon responses},
  Author                   = {Wu, Jing Qin and Dwyer, Dominic E. and Dyer, Wayne B. and Yang, Yee Hwa and Wang, Bin and Saksena, Nitin K.},

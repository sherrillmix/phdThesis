@Article{Weber2006,
  Title                    = {{L}iver-related deaths in persons infected with the human immunodeficiency virus: the {D:A:D} study},
  Author                   = {Rainer Weber and Caroline A Sabin and Nina Friis-Møller and Peter Reiss and Wafaa M El-Sadr and Ole Kirk and Francois Dabis and Matthew G Law and Christian Pradier and Stephane De Wit and Börje Akerlund and Gonzalo Calvo and Antonella d'Arminio Monforte and Martin Rickenbach and Bruno Ledergerber and Andrew N Phillips and Jens D Lundgren},
  Journal                  = {Arch Intern Med},
  Year                     = {2006},
  Number                   = {15},
  Pages                    = {1632--1641},
  Volume                   = {166},

  Abstract                 = {An increasing proportion of deaths among human immunodeficiency virus (HIV)-infected persons with access to combination antiretroviral therapy (cART) are due to complications of liver diseases.We investigated the frequency of and risk factors associated with liver-related deaths in the Data Collection on Adverse Events of Anti-HIV Drugs study, which prospectively evaluated 76 893 person-years of follow-up in 23 441 HIV-infected persons. Multivariable Poisson regression analyses identified factors associated with liver-related, AIDS-related, and other causes of death.There were 1246 deaths (5.3\%; 1.6 per 100 person-years); 14.5\% were from liver-related causes. Of these, 16.9\% had active hepatitis B virus (HBV), 66.1\% had hepatitis C virus (HCV), and 7.1\% had dual viral hepatitis co-infections. Predictors of liver-related deaths were latest CD4 cell count (adjusted relative rate [RR], 16.1; 95\% confidence interval [CI], 8.1-31.7 for <50 vs > or =500/microL), age (RR, 1.3; 95\% CI, 1.2-1.4 per 5 years older), intravenous drug use (RR, 2.0; 95\% CI, 1.2-3.4), HCV infection (RR, 6.7; 95\% CI, 4.0-11.2), and active HBV infection (RR, 3.7; 95\% CI, 2.4-5.9). Univariable analyses showed no relationship between cumulative years patients were receiving cART and liver-related death (RR, 1.00; 95\% CI, 0.93-1.07). Adjustment for the most recent CD4 cell count and patient characteristics resulted in an increased risk of liver-related mortality per year of mono or dual antiretroviral therapy before cART (RR, 1.09; 95\% CI, 1.02-1.16; P = .008) and per year of cART (RR, 1.11; 95\% CI, 1.02-1.21; P = .02).Liver-related death was the most frequent cause of non-AIDS-related death. We found a strong association between immunodeficiency and risk of liver-related death. Longer follow-up is required to investigate whether clinically significant treatment-associated liver-related mortality will develop.},
  Doi                      = {10.1001/archinte.166.15.1632},
  File                     = {:Weber2006.pdf:PDF},
  Institution              = {Division of Infectious Diseases and Hospital Epidemiology, University Hospital, CH-809a Zurich, Switzerland. infweb@usz.unizh.ch},
  Keywords                 = {Adult; Aged; Aged, 80 and over; Anti-HIV Agents, administration /&/ dosage/adverse effects; Antiretroviral Therapy, Highly Active, adverse effects; CD4 Lymphocyte Count; Drug-Induced Liver Injury; Female; HIV Infections, complications/immunology; Humans; Immunocompromised Host; Liver Diseases, complications/mortality; Male; Middle Aged; Prospective Studies; Risk Factors liver disease hcv hiv-1 treatment death dad study big HAART CD4 count},
  Language                 = {eng},

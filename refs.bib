  Url                      = {http://dx.doi.org/10.1086/342099}
}

@Article{Tan2012,
  Title                    = {{T}iling genomes of pathogenic viruses identifies potent antiviral sh{RNA}s and reveals a role for secondary structure in sh{RNA} efficacy},
  Author                   = {Xu Tan and Zhi John Lu and Geng Gao and Qikai Xu and Long Hu and Christof Fellmann and Mamie Z Li and Hongjing Qu and Scott W Lowe and Gregory J Hannon and Stephen J Elledge},

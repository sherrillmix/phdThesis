  Owner                    = {scott},
  Pii                      = {S0168-9525(11)00187-9},
  Pmid                     = {22197521},
  Timestamp                = {2012.02.03},
  Url                      = {http://dx.doi.org/10.1016/j.tig.2011.11.003}
}


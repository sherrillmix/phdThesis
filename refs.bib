  Timestamp                = {2013.01.22},
  Url                      = {http://dx.doi.org/10.1146/annurev.med.58.072905.180040}
}

@Article{Monette2009,
  Title                    = {{H}uman immunodeficiency virus type 1 ({HIV}-1) induces the cytoplasmic retention of heterogeneous nuclear ribonucleoprotein {A1} by disrupting nuclear import: implications for {HIV}-1 gene expression},
  Author                   = {Monette, Anne and Ajamian, Lara and L{\'{o}}pez-Lastra, Marcelo and Mouland, Andrew J.},
  Journal                  = {J Biol Chem},
  Year                     = {2009},

  Month                    = {Nov},
  Number                   = {45},
  Pages                    = {31350--31362},
  Volume                   = {284},

  Abstract                 = {Human immunodeficiency virus type 1 (HIV-1) co-opts host proteins and cellular machineries to its advantage at every step of the replication cycle. Here we show that HIV-1 enhances heterogeneous nuclear ribonucleoprotein (hnRNP) A1 expression and promotes the relocalization of hnRNP A1 to the cytoplasm. The latter was dependent on the nuclear export of the unspliced viral genomic RNA (vRNA) and to alterations in the abundance and localization of the FG-repeat nuclear pore glycoprotein p62. hnRNP A1 and vRNA remain colocalized in the cytoplasm supporting a post-nuclear function during the late stages of HIV-1 replication. Consistently, we show that hnRNP A1 acts as an internal ribosomal entry site trans-acting factor up-regulating internal ribosome entry site-mediated translation initiation of the HIV-1 vRNA. The up-regulation and cytoplasmic retention of hnRNP A1 by HIV-1 would ensure abundant expression of viral structural proteins in cells infected with HIV-1.},
  Doi                      = {10.1074/jbc.M109.048736},
  File                     = {:Monette2009.pdf:PDF},

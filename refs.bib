@Article{Korber2000,
  Title                    = {{T}iming the ancestor of the {HIV}-1 pandemic strains},
  Author                   = {Korber, B. and Muldoon, M. and Theiler, J. and Gao, F. and Gupta, R. and Lapedes, A. and Hahn, B. H. and Wolinsky, S. and Bhattacharya, T.},

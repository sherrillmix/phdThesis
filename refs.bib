@Article{DCC1996,
  Title                    = {{D}elta: a randomised double-blind controlled trial comparing combinations of zidovudine plus didanosine or zalcitabine with zidovudine alone in {HIV}-infected individuals},
  Author                   = {{Delta Coordinating Committee}},

  Author                   = {Choe, H. and Farzan, M. and Sun, Y. and Sullivan, N. and Rollins, B. and Ponath, P. D. and Wu, L. and Mackay, C. R. and LaRosa, G. and Newman, W. and Gerard, N. and Gerard, C. and Sodroski, J.},
  Journal                  = {Cell},

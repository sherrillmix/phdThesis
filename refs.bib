@Article{O'Reilly1995,
  Title                    = {{T}wo strong 5' splice sites and competing, suboptimal 3' splice sites involved in alternative splicing of human immunodeficiency virus type 1 {RNA}},
  Author                   = {M. M. O'Reilly and M. T. McNally and K. L. Beemon},

  Author                   = {Hahn, B. H. and Shaw, G. M. and Taylor, M. E. and Redfield, R. R. and Markham, P. D. and Salahuddin, S. Z. and Wong-Staal, F. and Gallo, R. C. and Parks, E. S. and Parks, W. P.},
  Journal                  = {Science},

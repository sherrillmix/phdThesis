@Article{Cillo2014,
  Title                    = {{Q}uantification of {HIV}-1 latency reversal in resting {CD}4+ {T} cells from patients on suppressive antiretroviral therapy},
  Author                   = {Cillo, Anthony R. and Sobolewski, Michele D. and Bosch, Ronald J. and Fyne, Elizabeth and Piatak, Jr, Michael and Coffin, John M. and Mellors, John W.},

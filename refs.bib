@Article{Carrera2010,
  Title                    = {{I}dentification of unusual and novel {HIV} type 1 spliced transcripts generated in vivo},
  Author                   = {Carrera, Cristina and Pinilla, Milagros and P{\'{e}}rez-Alvarez, Luc{\'{\i}}a and Thomson, Michael M.},

  Author                   = {Fischl, M. A. and Richman, D. D. and Causey, D. M. and Grieco, M. H. and Bryson, Y. and Mildvan, D. and Laskin, O. L. and Groopman, J. E. and Volberding, P. A. and Schooley, R. T.},
  Journal                  = {JAMA},
  Year                     = {1989},

  Month                    = {Nov},
  Number                   = {17},
  Pages                    = {2405--2410},
  Volume                   = {262},

  Abstract                 = {We examined the long-term safety and efficacy of zidovudine therapy in 229 subjects with acquired immunodeficiency syndrome (AIDS) and AIDS-related complex who previously participated in a placebo-controlled study of zidovudine. One hundred two placebo recipients (delayed treatment group) and 127 zidovudine recipients (original treatment group) were followed up while receiving zidovudine therapy for a mean of 21 months. Survival rates for the original treatment group were 84.5\% and 57.6\% at 12 and 21 months, respectively; for the delayed treatment group, 78.8\% and 64.6\% at 12 and 21 months, respectively, and 78.8\% and 47.5\% at 12 and 21 months, respectively, for 77 subjects with AIDS and 93.0\% and 71.8\%, respectively, for 50 subjects with AIDS-related complex in the original treatment group. Adverse reactions decreased over time and newly observed toxic reactions were unusual. The clinical course of these subjects suggests continued survival benefits with long-term zidovudine therapy.},
  Doi                      = {10.1001/jama.1989.03430170067030},
  File                     = {:Fischl1989.pdf:PDF},
  Institution              = {Department of Medicine, University of Miami School of Medicine, FL 33101.},

@Article{Bennasser2005,
  Title                    = {{E}vidence that {HIV}-1 encodes an si{RNA} and a suppressor of {RNA} silencing},
  Author                   = {Yamina Bennasser and Shu-Yun Le and Monsef Benkirane and Kuan-Teh Jeang},

}

@Article{Rerks-Ngarm2013,
  Title                    = {{E}xtended evaluation of the virologic, immunologic, and clinical course of volunteers who acquired {HIV}-1 infection in a phase {III} vaccine trial of {ALVAC}-{HIV} and {AIDSVAX} {B}/{E}},

  Owner                    = {shescott},
  Pii                      = {btp505},
  Pmid                     = {19692554},
  Timestamp                = {2010.01.06},
  Url                      = {http://dx.doi.org/10.1093/bioinformatics/btp505}
}

@Article{Gifford2012,

@Article{Ling2003,
  Title                    = {{T}he {ERV-9} {LTR} enhancer is not blocked by the {HS5} insulator and synthesizes through the {HS5} site non-coding, long {RNAs} that regulate {LTR} enhancer function},
  Author                   = {Ling, Jianhua and Pi, Wenhu and Yu, Xiuping and Bengra, Chikh and Long, Qiaoming and Jin, Huaqian and Seyfang, Andreas and Tuan, Dorothy},

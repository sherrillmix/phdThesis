@Article{Yusim2001,
  Title                    = {{U}sing human immunodeficiency virus type 1 sequences to infer historical features of the acquired immune deficiency syndrome epidemic and human immunodeficiency virus evolution},
  Author                   = {Yusim, K. and Peeters, M. and Pybus, O. G. and Bhattacharya, T. and Delaporte, E. and Mulanga, C. and Muldoon, M. and Theiler, J. and Korber, B.},
  Journal                  = {Philos Trans R Soc Lond B Biol Sci},
  Year                     = {2001},

  Month                    = {Jun},
  Number                   = {1410},
  Pages                    = {855--866},
  Volume                   = {356},

  Abstract                 = {In earlier work, human immunodeficiency virus type 1 (HIV-1) sequences were analysed to estimate the timing of the ancestral sequence of the main group of HIV-1, the virus that is responsible for the acquired immune deficiency syndrome pandemic, yielding a best estimate of 1931 (95\% confidence interval of 1915-1941). That work will be briefly reviewed, outlining how phylogenetic tools were extended to incorporate improved evolutionary models, how the molecular clock model was adapted to incorporate variable periods of latency, and how the approach was validated by correctly estimating the timing of two historically documented dates. The advantages, limitations, and assumptions of the approach will be summarized, with particular consideration of the implications of branch length uncertainty and recombination. We have recently undertaken new phylogenetic analysis of an extremely diverse set of human immunodeficiency virus envelope sequences from the Democratic Republic of the Congo (the DRC, formerly Zaire). This analysis both corroborates and extends the conclusions of our original study. Coalescent methods were used to infer the demographic history of the HIV-1 epidemic in the DRC, and the results suggest an increase in the exponential growth rate of the infected population through time.},
  Doi                      = {10.1098/rstb.2001.0859},
  File                     = {:Yusim2001.pdf:PDF},
  Institution              = {Los Alamos National Laboratory, Los Alamos, PO Box 1663, NM 87545, USA.},
  Keywords                 = {Acquired Immunodeficiency Syndrome, epidemiology/virology; Biological Evolution; Democratic Republic of the Congo, epidemiology; HIV-1, physiology; Humans; Models, Molecular; Monte Carlo Method; Phylogeny; Recombination, Genetic; Simian immunodeficiency virus, physiology; Time Factors phylogenetic clock common ancester recent most MRCA 1900s methods additional data HIV-1 epidemic},
  Language                 = {eng},

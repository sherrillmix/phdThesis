@Article{Dayton1986,
  Title                    = {{T}he trans-activator gene of the human {T} cell lymphotropic virus type {III} is required for replication},
  Author                   = {Dayton, A. I. and Sodroski, J. G. and Rosen, C. A. and Goh, W. C. and Haseltine, W. A.},

@Article{Kohlstaedt1992,
  Title                    = {{C}rystal structure at 3.5 {A} resolution of {HIV}-1 reverse transcriptase complexed with an inhibitor},
  Author                   = {Kohlstaedt, L. A. and Wang, J. and Friedman, J. M. and Rice, P. A. and Steitz, T. A.},

}

@Article{Patel2012,
  Title                    = {{S}ynergistic effects of interleukin-7 and pre-{T} cell receptor signaling in human {T} cell development},
  Author                   = {Patel, Ekta S. and Chang, Lung-Ji},
  Journal                  = {J Biol Chem},
  Year                     = {2012},

  Month                    = {Sep},
  Number                   = {40},
  Pages                    = {33826--33835},
  Volume                   = {287},

  Abstract                 = {The role of IL-7 in pre-T cell receptor (TCR) signaling during human T cell development is poorly understood. To study this, we engineered Molt3, a T cell progenitor T-acute lymphoblastic leukemia cell line, using lentiviral IL-7 receptor α (IL-7Rα) to serve as a model system. IL-7 promoted pre-TCR activation in IL-7Rα(hi) Molt3 as illustrated by CD25 up-regulation after anti-CD3 stimulation. Anti-CD3 treatment activated Akt and Erk1/2 signaling pathways as proven using specific inhibitors, and IL-7 further enhanced both signaling pathways. The close association of IL-7Rα with CD3ζ in the pre-TCR complex was illustrated through live imaging confocal fluorescence microscopy. These results demonstrate a direct and cooperative role of IL-7 in pre-TCR signaling.},

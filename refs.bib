  Author                   = {Hoenen, T. and Safronetz, D. and Groseth, A. and Wollenberg, K. R. and Koita, O. A. and Diarra, B. and Fall, I. S. and Haidara, F. C. and Diallo, F. and Sanogo, M. and Sarro, Y. S. and Kone, A. and Togo, A C G. and Traore, A. and Kodio, M. and Dosseh, A. and Rosenke, K. and {de Wit}, E. and Feldmann, F. and Ebihara, H. and Munster, V. J. and Zoon, K. C. and Feldmann, H. and Sow, S.},
  Journal                  = {Science},

@Article{Serquina2013,
  Title                    = {{UPF}1 is crucial for the infectivity of human immunodeficiency virus type 1 progeny virions},
  Author                   = {Serqui{\~{n}}a, Anna Kristina P. and Das, Suman R. and Popova, Elena and Ojelabi, Ogooluwa A. and Roy, Christian K. and G{\"{o}}ttlinger, Heinrich G.},
  Journal                  = {J Virol},
  Year                     = {2013},

  Month                    = {Aug},
  Number                   = {16},
  Pages                    = {8853--8861},
  Volume                   = {87},

  Abstract                 = {The SF1 helicase MOV10 is an antiviral factor that is incorporated into human immunodeficiency virus type 1 (HIV-1) virions. We now report that HIV-1 virions also incorporate UPF1, which belongs to the same SF1 helicase subfamily as MOV10 and functions in the nonsense-mediated decay (NMD) pathway. Unlike ectopic MOV10, the overexpression of UPF1 does not impair the infectivity of HIV-1 progeny virions. However, UPF1 becomes a potent inhibitor of HIV-1 progeny virion infectivity when residues required for its helicase activity are mutated. In contrast, equivalent mutations abolish the antiviral activity of MOV10. Importantly, cells depleted of endogenous UPF1, but not of another NMD core component, produce HIV-1 virions of substantially lower specific infectivity. The defect is at the level of reverse transcription, the same stage of the HIV-1 life cycle inhibited by ectopic MOV10. Thus, whereas ectopic MOV10 restricts HIV-1 replication, the related UPF1 helicase functions as a cofactor at an early postentry step.},
  Doi                      = {10.1128/JVI.00925-13},
  File                     = {:Serquina2013.pdf:PDF},
  Institution              = {Program in Gene Function and Expression, University of Massachusetts Medical School, Worcester, Massachusetts, USA.},
  Keywords                 = {HIV-1, physiology; Host-Pathogen Interactions; Humans; Mutant Proteins, genetics/metabolism; Trans-Activators, genetics/metabolism; Virus Replication upf1 hiv-1 nmd nonsense mediated decay mov10 cofactor repress helicase},
  Language                 = {eng},

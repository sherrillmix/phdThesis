}

@Article{Arya1985,
  Title                    = {{T}rans-activator gene of human {T}-lymphotropic virus type {III} ({HTLV}-{III})},
  Author                   = {Arya, S. K. and Guo, C. and Josephs, S. F. and Wong-Staal, F.},

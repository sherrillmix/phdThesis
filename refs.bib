@Article{Cicala2002,
  Title                    = {{HIV} envelope induces a cascade of cell signals in non-proliferating target cells that favor virus replication},
  Author                   = {Claudia Cicala and James Arthos and Sara M Selig and Glynn Dennis and Douglas A Hosack and Donald Van Ryk and Marion L Spangler and Tavis D Steenbeke and Prateeti Khazanie and Neil Gupta and Jun Yang and Marybeth Daucher and Richard A Lempicki and Anthony S Fauci},

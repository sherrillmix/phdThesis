}

@Article{Payne1994,
  Title                    = {{C}haracterization of infectious molecular clones of equine infectious anaemia virus},
  Author                   = {S. L. Payne and J. Rausch and K. Rushlow and R. C. Montelaro and C. Issel and M. Flaherty and S. Perry and D. Sellon and F. Fuller},
  Journal                  = {J Gen Virol},
  Year                     = {1994},


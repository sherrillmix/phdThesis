  Institution              = {Department of Chemical Engineering, University of Wisconsin-Madison, 53706-1691, USA.},
  Keywords                 = {CD4-Positive T-Lymphocytes, virology; HIV Infections, immunology/virology; HIV-1, physiology; Humans; Kinetics; Models, Biological; Virus Replication HIV model kinetics growth CD4+ T lymphocyte assembly splicing reverse transcription parameters predictions math},

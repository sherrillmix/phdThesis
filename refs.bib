  Author                   = {Schaller, Torsten and Ocwieja, Karen E. and Rasaiyaah, Jane and Price, Amanda J. and Brady, Troy L. and Roth, Shoshannah L. and Hu{\'{e}}, St{\'{e}}phane and Fletcher, Adam J. and Lee, KyeongEun and KewalRamani, Vineet N. and Noursadeghi, Mahdad and Jenner, Richard G. and James, Leo C. and Bushman, Frederic D. and Towers, Greg J.},
  Journal                  = {PLoS Pathog},
  Year                     = {2011},

  Month                    = {Dec},
  Number                   = {12},
  Pages                    = {e1002439},
  Volume                   = {7},

  Abstract                 = {Lentiviruses such as HIV-1 traverse nuclear pore complexes (NPC) and infect terminally differentiated non-dividing cells, but how they do this is unclear. The cytoplasmic NPC protein Nup358/RanBP2 was identified as an HIV-1 co-factor in previous studies. Here we report that HIV-1 capsid (CA) binds directly to the cyclophilin domain of Nup358/RanBP2. Fusion of the Nup358/RanBP2 cyclophilin (Cyp) domain to the tripartite motif of TRIM5 created a novel inhibitor of HIV-1 replication, consistent with an interaction in vivo. In contrast to CypA binding to HIV-1 CA, Nup358 binding is insensitive to inhibition with cyclosporine, allowing contributions from CypA and Nup358 to be distinguished. Inhibition of CypA reduced dependence on Nup358 and the nuclear basket protein Nup153, suggesting that CypA regulates the choice of the nuclear import machinery that is engaged by the virus. HIV-1 cyclophilin-binding mutants CA G89V and P90A favored integration in genomic regions with a higher density of transcription units and associated features than wild type virus. Integration preference of wild type virus in the presence of cyclosporine was similarly altered to regions of higher transcription density. In contrast, HIV-1 CA alterations in another patch on the capsid surface that render the virus less sensitive to Nup358 or TRN-SR2 depletion (CA N74D, N57A) resulted in integration in genomic regions sparse in transcription units. Both groups of CA mutants are impaired in replication in HeLa cells and human monocyte derived macrophages. Our findings link HIV-1 engagement of cyclophilins with both integration targeting and replication efficiency and provide insight into the conservation of viral cyclophilin recruitment.},
  Doi                      = {10.1371/journal.ppat.1002439},
  File                     = {:Schaller2011.pdf:PDF},
  Institution              = {Division of Infection and Immunity, University College London Medical Research Council Centre for Medical Molecular Virology, London, United Kingdom.},
  Keywords                 = {Active Transport, Cell Nucleus, physiology; Blotting, Western; Capsid Proteins, metabolism; Cell Line; Cell Nucleus, metabolism/virology; Cyclophilin A, metabolism; HIV Infections, metabolism; HIV-1, physiology; HeLa Cells; Humans; Macrophages, metabolism/virology; Molecular Chaperones, metabolism; Nuclear Pore Complex Proteins, metabolism; Reverse Transcriptase Polymerase Chain Reaction; Virus Replication, physiology cyclophilin HIV-1 capsid interaction nuclear pore integration targeting},
  Language                 = {eng},
  Medline-pst              = {ppublish},
  Owner                    = {scott},
  Pii                      = {PPATHOGENS-D-11-01130},
  Pmid                     = {22174692},
  Timestamp                = {2015.05.24 13:44},
  Url                      = {http://dx.doi.org/10.1371/journal.ppat.1002439}
}

@Article{Schaub2007,

  Timestamp                = {2011.03.25},
  Url                      = {http://www.pnas.org/content/84/23/8453}
}

@Article{Kulkarni2014,
  Title                    = {{T}he combined anti-{HIV}-1 activities of emtricitabine and tenofovir plus the integrase inhibitor elvitegravir or raltegravir show high levels of synergy in vitro},
  Author                   = {Kulkarni, Rima and Hluhanich, Rebecca and McColl, Damian M. and Miller, Michael D. and White, Kirsten L.},

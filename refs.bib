@Article{Cassol2006,
  Title                    = {{M}onocyte-derived macrophages and myeloid cell lines as targets of {HIV}-1 replication and persistence},
  Author                   = {Edana Cassol and Massimo Alfano and Priscilla Biswas and Guido Poli},
  Journal                  = {J Leukoc Biol},

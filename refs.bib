  Owner                    = {scott},
  Pmid                     = {1696639},
  Timestamp                = {2014.06.30},
  Url                      = {http://jvi.asm.org/content/64/9/4321.long}
}

@Article{Lusic2013,
  Title                    = {{P}roximity to {PML} nuclear bodies regulates {HIV}-1 latency in {CD4}+ {T} cells},
  Author                   = {Marina Lusic and Bruna Marini and Hashim Ali and Bojana Lucic and Roberto Luzzati and Mauro Giacca},

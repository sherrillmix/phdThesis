  Language                 = {eng},
  Medline-pst              = {ppublish},
  Owner                    = {scott},
  Pii                      = {S1074-7613(05)00105-6},
  Pmid                     = {15894278},
  Timestamp                = {2012.03.23},
  Url                      = {http://dx.doi.org/10.1016/j.immuni.2005.03.010}

  Timestamp                = {2012.03.26},
  Url                      = {http://dx.doi.org/10.1074/jbc.M109.077453}
}

@Article{Tranell2011,
  Title                    = {{I}nhibition of splicing by serine-arginine rich protein 55 ({SRp55}) causes the appearance of partially spliced {HIV}-1 m{RNA}s in the cytoplasm},
  Author                   = {Anna Tranell and Susanne Tingsborg and Eva Maria Fenyö and Stefan Schwartz},

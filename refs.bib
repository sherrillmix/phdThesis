@Article{Halper-Stromberg2014,
  Title                    = {{B}roadly neutralizing antibodies and viral inducers decrease rebound from {HIV}-1 latent reservoirs in humanized mice},
  Author                   = {Halper-Stromberg, Ariel and Lu, Ching-Lan and Klein, Florian and Horwitz, Joshua A. and Bournazos, Stylianos and Nogueira, Lilian and Eisenreich, Thomas R. and Liu, Cassie and Gazumyan, Anna and Schaefer, Uwe and Furze, Rebecca C. and Seaman, Michael S. and Prinjha, Rab and Tarakhovsky, Alexander and Ravetch, Jeffrey V. and Nussenzweig, Michel C.},
  Journal                  = {Cell},
  Year                     = {2014},

  Month                    = {Aug},
  Number                   = {5},
  Pages                    = {989--999},
  Volume                   = {158},

  Abstract                 = {Latent reservoirs of HIV-1-infected cells are refractory to antiretroviral therapies (ART) and remain the major barrier to curing HIV-1. Because latently infected cells are long-lived, immunologically invisible, and may undergo homeostatic proliferation, a "shock and kill" approach has been proposed to eradicate this reservoir by combining ART with inducers of viral transcription. However, all attempts to alter the HIV-1 reservoir in vivo have failed to date. Using humanized mice, we show that broadly neutralizing antibodies (bNAbs) can interfere with establishment of a silent reservoir by Fc-FcR-mediated mechanisms. In established infection, bNAbs or bNAbs plus single inducers are ineffective in preventing viral rebound. However, bNAbs plus a combination of inducers that act by independent mechanisms synergize to decrease the reservoir as measured by viral rebound. Thus, combinations of inducers and bNAbs constitute a therapeutic strategy that impacts the establishment and maintenance of the HIV-1 reservoir in humanized mice.},
  Doi                      = {10.1016/j.cell.2014.07.043},
  File                     = {:Halper-Stromberg2014.pdf:PDF},
  Institution              = {Laboratory of Molecular Immunology, The Rockefeller University, New York, NY 10065, USA; Howard Hughes Medical Institute. Electronic address: nussen@rockefeller.edu.},
  Keywords                 = {Animals; Anti-HIV Agents, therapeutic use; Antibodies, Neutralizing, administration /&/ dosage/immunology; CD4-Positive T-Lymphocytes, immunology; CTLA-4 Antigen, administration /&/ dosage; HIV Infections, immunology/virology; HIV-1, drug effects/genetics/physiology; Heterocyclic Compounds with 4 or More Rings, administration /&/ dosage; Humans; Hydroxamic Acids, administration /&/ dosage; Immunoglobulin Fc Fragments, immunology; Mice; Receptors, Fc, immunology; Transcription, Genetic, drug effects; Virus Latency, drug effects induction combination BET T cell activation histone deacetylase HDACi drug treatment antibody therapy},
  Language                 = {eng},

@Article{Vargas2011,
  Title                    = {{S}ingle-molecule imaging of transcriptionally coupled and uncoupled splicing},
  Author                   = {Diana Y Vargas and Khyati Shah and Mona Batish and Michael Levandoski and Sourav Sinha and Salvatore A E Marras and Paul Schedl and Sanjay Tyagi},

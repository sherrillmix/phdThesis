@Article{Watson2005,
  Title                    = {{E}xtensive diversity of {Ig}-superfamily proteins in the immune system of insects},
  Author                   = {Fiona L Watson and Roland Püttmann-Holgado and Franziska Thomas and David L Lamar and Michael Hughes and Masahiro Kondo and Vivienne I Rebel and Dietmar Schmucker},

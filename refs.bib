@Article{Farnham2013,
  Title                    = {{U}pdates of lifetime costs of care and quality-of-life estimates for {HIV}-infected persons in the {U}nited {S}tates: late versus early diagnosis and entry into care},
  Author                   = {Farnham, Paul G. and Gopalappa, Chaitra and Sansom, Stephanie L. and Hutchinson, Angela B. and Brooks, John T. and Weidle, Paul J. and Marconi, Vincent C. and Rimland, David},

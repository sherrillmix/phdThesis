  Institution              = {Department of Biochemistry and Molecular Biology (MSB 2370), University of South Alabama, College of Medicine, 307 University Blvd, Mobile, AL 36688-0002, USA. vbitko@jaguar1.usouthal.edu},
  Keywords                 = {Gene Silencing, drug effects; Phenotype; RNA Viruses, drug effects/genetics; RNA, Double-Stranded, pharmacology; RNA, Viral, drug effects/genetics/metabolism; Respiratory Syncytial Viruses, drug effects/genetics, old siRNA RNAi interference RNA virus RSV},
  Language                 = {eng},
  Medline-pst              = {ppublish},
  Owner                    = {scott},
  Pmid                     = {11801185},
  Timestamp                = {2012.03.23},
  Url                      = {http://dx.doi.org/10.1186/1471-2180-1-34}
}


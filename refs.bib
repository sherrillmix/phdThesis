  Author                   = {Dornsife, R. E. and {St Clair}, M. H. and Huang, A. T. and Panella, T. J. and Koszalka, G. W. and Burns, C. L. and Averett, D. R.},
  Journal                  = {Antimicrob Agents Chemother},
  Year                     = {1991},

  Month                    = {Feb},
  Number                   = {2},

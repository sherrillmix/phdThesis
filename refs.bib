  Pii                      = {PPATHOGENS-D-13-01509},
  Pmid                     = {24651464},
  Timestamp                = {2015.03.18},
  Url                      = {http://dx.doi.org/10.1371/journal.ppat.1004010}
}

@Article{Keele2008,

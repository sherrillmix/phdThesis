@Article{Bukrinsky1993,
  Title                    = {{A}ssociation of integrase, matrix, and reverse transcriptase antigens of human immunodeficiency virus type 1 with viral nucleic acids following acute infection},
  Author                   = {Bukrinsky, M. I. and Sharova, N. and McDonald, T. L. and Pushkarskaya, T. and Tarpley, W. G. and Stevenson, M.},

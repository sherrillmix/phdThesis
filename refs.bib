  Pmid                     = {24089548},
  Timestamp                = {2015.03.18},
  Url                      = {http://dx.doi.org/10.1128/JVI.02257-13}
}

@Article{Joos2008,
  Title                    = {{HIV} rebounds from latently infected cells, rather than from continuing low-level replication},
  Author                   = {Joos, Beda and Fischer, Marek and Kuster, Herbert and Pillai, Satish K. and Wong, Joseph K. and B{\"{o}}ni, J{\"{u}}rg and Hirschel, Bernard and Weber, Rainer and Trkola, Alexandra and G{\"{u}}nthard, Huldrych F. and , Swiss H. I. V Cohort Study},
  Journal                  = {Proc Natl Acad Sci U S A},
  Year                     = {2008},

  Month                    = {Oct},
  Number                   = {43},
  Pages                    = {16725--16730},
  Volume                   = {105},

  Abstract                 = {Rapid rebound of plasma viremia in patients after interruption of long-term combination antiretroviral therapy (cART) suggests persistence of low-level replicating cells or rapid reactivation of latently infected cells. To further characterize rebounding virus, we performed extensive longitudinal clonal evolutionary studies of HIV env C2-V3-C3 regions and exploited the temporal relationships of rebounding plasma viruses with regard to pretreatment sequences in 20 chronically HIV-1-infected patients having undergone multiple 2-week structured treatment interruptions (STI). Rebounding virus during the short STI was homogeneous, suggesting mono- or oligoclonal origin during reactivation. No evidence for a temporal structure of rebounding virus in regard to pretreatment sequences was found. Furthermore, expansion of distinct lineages at different STI cycles emerged. Together, these findings imply stochastic reactivation of different clones from long-lived latently infected cells rather than expansion of viral populations replicating at low levels. After treatment was stopped, diversity increased steadily, but pretreatment diversity was, on average, achieved only >2.5 years after the start of STI when marked divergence from preexisting quasispecies also emerged. In summary, our results argue against persistence of ongoing low-level replication in patients on suppressive cART. Furthermore, a prolonged delay in restoration of pretreatment viral diversity after treatment interruption demonstrates a surprisingly sustained evolutionary bottleneck induced by punctuated antiretroviral therapy.},
  Doi                      = {10.1073/pnas.0804192105},
  File                     = {:Joos2008.pdf:PDF},
  Institution              = {Department of Medicine, Division of Infectious Diseases and Hospital Epidemiology, University Hospital Zürich, CH-8091 Zurich, Switzerland. beda.joos@usz.ch},
  Keywords                 = {Cells, Cultured; Genetic Variation; HIV Infections, drug therapy/virology; HIV, genetics/physiology; Humans; Molecular Sequence Data; Phylogeny; Receptors, CXCR4; Receptors, HIV; Virus Activation; Virus Latency, drug effects; Virus Replication, drug effects long lived latent reservoir evolution lack antiretroviral patients HAART long term stemness hiv-1 patients},
  Language                 = {eng},
  Medline-pst              = {ppublish},
  Owner                    = {scott},
  Pii                      = {0804192105},
  Pmid                     = {18936487},
  Timestamp                = {2015.03.18},
  Url                      = {http://dx.doi.org/10.1073/pnas.0804192105}
}

@Article{Jordan2003,
  Title                    = {{HIV} reproducibly establishes a latent infection after acute infection of {T} cells in vitro},
  Author                   = {Albert Jordan and Dwayne Bisgrove and Eric Verdin},
  Journal                  = {EMBO J},
  Year                     = {2003},

  Month                    = {Apr},
  Number                   = {8},
  Pages                    = {1868--1877},
  Volume                   = {22},

  Abstract                 = {The presence of latent reservoirs has prevented the eradication of human immunodeficiency virus (HIV) from infected patients successfully treated with anti-retroviral therapy. The mechanism of postintegration latency is poorly understood, partly because of the lack of an in vitro model. We have used an HIV retroviral vector or a full-length HIV genome expressing green fluorescent protein to infect a T lymphocyte cell line in vitro and highly enrich for latently infected cells. HIV latency occurred reproducibly, albeit with low frequency, during an acute infection. Clonal cell lines derived from latent populations showed no detectable basal expression, but could be transcriptionally activated after treatment with phorbol esters or tumor necrosis factor alpha. Direct sequencing of integration sites demonstrated that latent clones frequently contain HIV integrated in or close to alphoid repeat elements in heterochromatin. This is in contrast to a productive infection where integration in or near heterochromatin is disfavored. These observations demonstrate that HIV can reproducibly establish a latent infection as a consequence of integration in or near heterochromatin.},
  Doi                      = {10.1093/emboj/cdg188},

@Article{Schindler2006,
  Title                    = {{N}ef-mediated suppression of {T} cell activation was lost in a lentiviral lineage that gave rise to {HIV}-1},
  Author                   = {Schindler, Michael and M{\"{u}}nch, Jan and Kutsch, Olaf and Li, Hui and Santiago, Mario L. and Bibollet-Ruche, Frederic and M{\"{u}}ller-Trutwin, Michaela C. and Novembre, Francis J. and Peeters, Martine and Courgnaud, Valerie and Bailes, Elizabeth and Roques, Pierre and Sodora, Donald L. and Silvestri, Guido and Sharp, Paul M. and Hahn, Beatrice H. and Kirchhoff, Frank},

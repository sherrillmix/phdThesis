@Article{Valente2006,
  Title                    = {{I}nhibition of {HIV}-1 gene expression by a fragment of {hnRNP} {U}},
  Author                   = {Susana T Valente and Stephen P Goff},
  Journal                  = {Mol Cell},
  Year                     = {2006},


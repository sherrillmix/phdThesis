  Author                   = {Masur, H. and Michelis, M. A. and Greene, J. B. and Onorato, I. and Stouwe, R. A. and Holzman, R. S. and Wormser, G. and Brettman, L. and Lange, M. and Murray, H. W. and Cunningham-Rundles, S.},
  Journal                  = {N Engl J Med},

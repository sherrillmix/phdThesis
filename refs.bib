  Timestamp                = {2012.08.08},
  Url                      = {http://dx.doi.org/10.1371/journal.pcbi.1002593}
}

@Article{Boer2010,
  Title                    = {{C}urrent estimates for {HIV}-1 production imply rapid viral clearance in lymphoid tissues},
  Author                   = {Rob J De Boer and Ruy M Ribeiro and Alan S Perelson},
  Journal                  = {PLoS Comput Biol},
  Year                     = {2010},
  Number                   = {9},
  Pages                    = {e1000906},
  Volume                   = {6},

  Abstract                 = {It has recently been estimated that a single HIV-1 infected cell produces between and more than viral particles over its life span. Since body-wide estimates of the ratio of free virus to productively infected cells are smaller than and much smaller than , individual virions must be cleared rapidly. This seems difficult to reconcile with the fact that most of the total body virus is trapped on follicular dendritic cells where it can survive for many months. It has also been difficult to reconcile the vast difference in the rates at which the virus is cleared from the blood in rhesus macaques and in chronically infected patients. Here we attempt to reconcile these seemingly contradictory observations by considering the virion clearance rate in various organs and the virion exchange rates between them. The main results are that the per capita clearance rate of free virus in lymphoid tissue should be fast, the virion exchange rate between lymphoid tissue and the blood should be slow, and the comparatively slow previous estimates for the virion clearance rate from the blood correspond to the rate of virion efflux from the blood to other organs where the virus is ultimately cleared.},
  Doi                      = {10.1371/journal.pcbi.1000906},
  File                     = {:Boer2010.pdf:PDF},

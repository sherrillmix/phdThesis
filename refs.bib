@Article{Strazzullo1994,
  Title                    = {{M}utational analysis of the human endogenous {ERV9} proviruses promoter region},
  Author                   = {Strazzullo, M. and Majello, B. and Lania, L. and {La Mantia}, G.},
  Journal                  = {Virology},
